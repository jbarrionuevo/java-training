package interfaces;

import java.util.Date;

import classes.Order;
import classes.Receipt;
import classes.Sale;
import classes.StoreOrder;

public interface StoreSellerInterface {
	
	
	default Sale sellCase(StoreOrder order){
		return new Sale(new Receipt("Fravega", order.getQuantity(), order.getOwner(),new Date(), order.getMyCase().getPrice() ), order.getMyCase());
//		return new Receipt("Fravega", order.getQuantity(), order.getOwner(),new Date(), order.getMyCase().getPrice() );
	}
}
