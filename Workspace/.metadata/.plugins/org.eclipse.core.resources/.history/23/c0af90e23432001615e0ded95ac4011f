package classes;

import java.util.Date;

public class Order {
	private Case myCase;
	private int quantity;
	private Customer owner;
	private Date date;
	
	public Order(){
		
	}
	
	public Order(Case myCase, int quantity, Customer owner, Date date) {
		super();
		this.myCase = myCase;
		this.quantity = quantity;
		this.setOwner(owner);
		this.date=date;
	}
	
	public Case getMyCase() {
		return myCase;
	}
	public void setMyCase(Case myCase) {
		this.myCase = myCase;
	}
	public int getQuantity() {
		return quantity;
	}
	public void setQuantity(int quantity) {
		this.quantity = quantity;
	}

	public Customer getOwner() {
		return owner;
	}

	public void setOwner(Customer owner) {
		this.owner = owner;
	}
	
	
}
