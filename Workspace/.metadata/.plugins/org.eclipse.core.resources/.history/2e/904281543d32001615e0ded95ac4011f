package classes;

import java.util.Date;

public class ProviderOrder extends Order{
	private Provider provider;
	
	public ProviderOrder(Case myCase, int quantity, Date dateOfRequest, int id, Provider provider){
		super(myCase,quantity,dateOfRequest, id);
		this.provider=provider;
	}

	public Provider getProvider() {
		return provider;
	}

	public void setProvider(Provider provider) {
		this.provider = provider;
	}
}
