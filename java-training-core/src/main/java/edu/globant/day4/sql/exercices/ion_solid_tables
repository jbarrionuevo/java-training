Tables to use ion ION_SOLID:

1) case_design
CREATE TABLE `case_design` (
  `id` bigint(20) NOT NULL,
  `name` varchar(255)
)


2) case_device
CREATE TABLE `case_device` (
  `id` bigint(20) NOT NULL,
  `name` varchar(255)
)


3) case_order
CREATE TABLE `case_order` (
  `id` bigint(20) NOT NULL,
  `date_of_delivery` datetime DEFAULT NULL,
  `date_of_request` datetime DEFAULT NULL,
  `request_cases` tinyblob  #this is because of the hashmap
) 


4) case_product
CREATE TABLE `case_product` (
  `id` bigint(20) NOT NULL,
  `price` double NOT NULL,
  `design_id` bigint(20) DEFAULT NULL,
  `device_id` bigint(20) DEFAULT NULL,
  `provider_id` bigint(20) DEFAULT NULL
) 

5) case_wrapper
CREATE TABLE `case_wrapper` (
  `id` int(11) NOT NULL,
  `current_stock` int(11) NOT NULL,
  `minimum_stock` int(11) NOT NULL,
  `my_case_id` bigint(20) NOT NULL,
  `inventory_id` int(11) NOT NULL
) 

6) employee
CREATE TABLE `employee` (
  `dtype` varchar(31) NOT NULL, #will determine if its a seller, logistic, etc.
  `id` bigint(20) NOT NULL,
  `name` varchar(255) DEFAULT NULL
)

7) inventory
CREATE TABLE `inventory` (
  `id` int(11) NOT NULL
)

8) receipt
CREATE TABLE `receipt` (
  `id` bigint(20) NOT NULL,
  `date_of_sale` datetime DEFAULT NULL,
  `store_name` varchar(255) DEFAULT NULL,
  `case_order_id` bigint(20) DEFAULT NULL,
  `customer_id` bigint(20) DEFAULT NULL
)

9) sale
CREATE TABLE `sale` (
  `id` bigint(20) NOT NULL,
  `status` int(11) DEFAULT NULL,
  `case_order_id` bigint(20) DEFAULT NULL,
  `receipt_id` bigint(20) DEFAULT NULL,
  `seller_id` bigint(20) DEFAULT NULL
)

10) third_party_participant
CREATE TABLE `third_party_participant` (
  `dtype` varchar(31) NOT NULL,
  `id` bigint(20) NOT NULL,
  `location` varchar(255) DEFAULT NULL,
  `name` varchar(255) DEFAULT NULL,
  `age` int(11) DEFAULT NULL,
  `gender` int(11) DEFAULT NULL
)

11) third_party_participant_orders
CREATE TABLE `third_party_participant_orders` (
  `third_party_participant_id` bigint(20) NOT NULL,
  `orders_id` bigint(20) NOT NULL
)


-- Indices de la tabla `case_design`
ALTER TABLE `case_design`
  ADD PRIMARY KEY (`id`),
  ADD KEY `case_design_name` (`name`);

-- Indices de la tabla `case_device`
ALTER TABLE `case_device`
  ADD PRIMARY KEY (`id`),
  ADD KEY `case_device_name` (`name`);

-- Indices de la tabla `case_order`
ALTER TABLE `case_order`
  ADD PRIMARY KEY (`id`),
  ADD KEY `case_order_date_of_request` (`date_of_request`),
  ADD KEY `case_order_date_of_delivery` (`date_of_delivery`);
etc..