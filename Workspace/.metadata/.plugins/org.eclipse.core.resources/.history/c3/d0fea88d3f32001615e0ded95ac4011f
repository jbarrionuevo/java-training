package interfaces;

import java.util.Date;

import classes.Order;
import classes.Receipt;
import classes.Sale;
import classes.StoreOrder;

public interface StoreSellerInterface {
	
	
	default void sellCase(Sale sale){
		sale.setReceipt(new Receipt("Fravega", sale.getOrder().getQuantity(), sale.getOrder().getOwner(),new Date(), sale.getOrder().getMyCase().getPrice()));
//		return new Receipt("Fravega", order.getQuantity(), order.getOwner(),new Date(), order.getMyCase().getPrice() );
	}
}
