package interfaces;

import classes.CaseSeller;
import classes.DeliverySeller;
import classes.Order;

public interface DeliverySellerInterface {
	
	default void deliver(CaseSeller me){
		me.getOrders().removeAll(me.getOrders());
	}
	
	default void attendRequest(CaseSeller me){
		Order newOrder = new Order();
		me.registerOrder(newOrder);
	}
	
}
